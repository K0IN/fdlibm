.c.o:
	$(CC) -c $(DEFS) -I$(srcdir) $(CFLAGS) $< $(OUTPUT_OPTION)

all:
	for i in $(CPU-FPU-TYPES); do \
	  if [ ! -d $$i ] ; then \
	    mkdir $$i; \
	  else true; fi; \
	  (cd $$i; make -f ../Makefile CPU-FPU=$$i src_dir=$(srcdir) $(LIB)); \
	done

LIB = libm.a

$(LIB): $(OBJ) 
	ar cru $(LIB) $(OBJ)
	$(RANLIB) $(LIB)

install: all
	if [ -d 68000.soft-float ] ; then \
	  $(srcdir)/mkinstalldirs $(libdir); \
	  cp 68000.soft-float/libm.a $(libdir)/libm.a; \
	else true; fi
	if [ -d 68020-60.soft-float ] ; then \
	  $(srcdir)/mkinstalldirs $(libdir)/libm020; \
	  cp 68020-60.soft-float/libm.a $(libdir)/libm020/libm.a; \
	else true; fi
