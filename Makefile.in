	  cp 68060.68881/libm.a $(libdir)/lib/libm060/libm881/libm.a; \
	else true; fi
	if [ -d 68020-60.soft-float ] ; then \
	  $(srcdir)/mkinstalldirs $(libdir)/lib32/libm020; \
